{
    "manifest_version": 3,
    "name": "Title Saver",
    "version": "1.0",
    "description": "Get the title of the video and save it in local storage.",
    "permissions": ["activeTab","tabs","storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
    
  
  }
  